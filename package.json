{
	"name": "@justinseibert/collidicon",
	"version": "1.0.0",
	"description": "a tool for generating beautiful hash-based identicons",
	"main": "./dist/index.js",
	"types": "./lib/index.d.ts",
	"scripts": {
		"clean": "rm -r ./dist/* ./lib/*",
		"types": "tsc && tscpaths -p tsconfig.json -s ./src -o ./lib",
		"build:pro": "webpack --config webpack.pro.js",
		"build": "npm run clean || npm run types && npm run build:pro",
		"start": "webpack serve --config webpack.dev.js",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.13.13",
		"@babel/preset-env": "^7.13.12",
		"@types/node": "^17.0.21",
		"autoprefixer": "^10.2.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"css-loader": "^5.2.0",
		"html-webpack-plugin": "^5.3.1",
		"postcss-loader": "^5.2.0",
		"sass": "^1.32.8",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.0.18",
		"tscpaths": "^0.0.9",
		"typescript": "^4.6.2",
		"webpack": "^5.28.0",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.7.4"
	},
	"dependencies": {
		"buffer": "^6.0.3",
		"crypto-browserify": "^3.12.0",
		"lil-gui": "^0.16.1",
		"stream-browserify": "^3.0.0"
	}
}
